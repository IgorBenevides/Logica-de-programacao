algoritmo "semnome"
var
   c ,s ,n :inteiro
inicio
      c<-0
      s<-0
      enquanto c < 7 faca
      escreva ("digite o numero a somar :")
      leia(n)
               s <- s+n
               c<- c+1
      fimenquanto
                 escreva ("A soma entre todos os numero é de :",s)
fimalgoritmo
