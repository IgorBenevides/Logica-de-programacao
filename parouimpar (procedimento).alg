algoritmo "semnome"
var
   n :inteiro
procedimento parouimpar (a :inteiro)
inicio
      se n%2 = 0 entao
         escreva ("O numero é par ")
      senao
           escreva ("O numero é impar")
      fimse
fimprocedimento
inicio
      escreva ("Digite um numero : ")
      leia (n)
      parouimpar (n)
fimalgoritmo
