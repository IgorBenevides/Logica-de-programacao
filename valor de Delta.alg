algoritmo "semnome"
var
   delta,a,b,c:real
inicio
      escreval ("Informe o valor das medidas")
      escreva ("medida A = ")
      leia (a)
      escreva ("medida B = ")
      leia (b)
      escreva ("medida C = ")
      leia (c)
      delta <- (b^2)-(4*a*c)
            escreva ("o valor de delta deste triangulo é igual a = ",delta)

fimalgoritmo

