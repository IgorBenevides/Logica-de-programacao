algoritmo "semnome"
var
   n1,n2,me:real
   Ma :caracter
inicio
      escreva ("informe o nome da  materia : ")
      leia (ma)
      escreva ("Qual a primeira nota : ")
      leia (n1)
      escreva ("Qual a segunda nota : ")
      leia (n2)
      me <- (n1+n2)/2
           escreva ("A média do aluno em ",ma," é  de : ",me)
      
fimalgoritmo
