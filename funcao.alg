algoritmo "semnome"
var
   n1 , n2 ,res :inteiro
funcao soma (a ,b :inteiro) :inteiro
var
   s :inteiro
inicio
      s <- a+b
      retorne s
fimfuncao
inicio
      n1 <- 4
      n2 <- 3
      res <- soma (n1,n2)
      escreva ("A soma de ",n1," +",n2," é :",res)
      res <- soma (7,5)
      escreva ("A soma de 7 + 5  é :",res)


      
fimalgoritmo
